<center>
  <h3>Users</h3>
  <table class="mdl-data-table mdl-js-data-table">
    <thead>
    <tr>
      <th>ID</th>
      <th class="mdl-data-table__cell--non-numeric">Name</th>
      <th class="mdl-data-table__cell--non-numeric">Username</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    </thead>
    <% _.each(users, function(user) { %>
    <tbody>
    <tr data-id="<%= user.id %>" data-model="user">
      <td><%= user.id %></td>
      <td class="mdl-data-table__cell--non-numeric"><%= user.name %></td>
      <td class="mdl-data-table__cell--non-numeric"><%= user.username %></td>
      <td><a href="/user/show/<%= user.id %>">Show</a></td>
      <td><a href="/user/edit/<%= user.id %>">Edit</a></td>
      <td><a href="/user/destroy/<%= user.id %>">Delete</a></td>
    </tr>
    </tbody>
    <% }) %>
  </table>
  <br/>
  <a href="/user/new">
    <button class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent">Sign up
    </button>
  </a>
</center>
